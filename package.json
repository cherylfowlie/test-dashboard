    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "express": "^4.16.3",
    "firebase": "^8.2.5",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "react": "^16.13.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
